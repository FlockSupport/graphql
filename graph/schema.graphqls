# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type User {
  id: Int!
  name: String!
  age: Int!
}

type Query {
  users: [User!]
  singleUser(input: IdInput!): User!
}

input IdInput {
  id: Int!
}


input NewUser {
  id: Int!
  name: String!
  age: Int!
}

type Mutation {
  createUser(input: NewUser!): User!
}